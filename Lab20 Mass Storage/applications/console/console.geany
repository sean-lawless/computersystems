[editor]
line_wrapping=false
line_break_column=72
auto_continue_multiline=true

[file_prefs]
final_new_line=true
ensure_convert_new_lines=true
strip_trailing_spaces=true
replace_tabs=true

[indentation]
indent_width=2
indent_type=0
indent_hard_tab_width=8
detect_indent=false
detect_indent_width=false
indent_mode=2

[project]
name=console
base_path=.
description=
file_patterns=

[long line marker]
long_line_behaviour=1
long_line_column=72

[files]
current_page=0

[build-menu]
NF_00_LB=_Make
NF_00_CM=make -j4
NF_00_WD=.
NF_01_LB=Make Custom _Target...
NF_01_CM=make -j4
NF_01_WD=
NF_03_LB=Make Clean
NF_03_CM=make clean
NF_03_WD=.
CFT_00_LB=_Compile
CFT_00_CM=arm-none-eabi-gcc -Wall -c -O0 -ggdb -DRPI=3 -ffreestanding -I"." -I"../../include" -I"../../boards/rpi" "%f"
CFT_00_WD=
CFT_01_LB=_Build
CFT_01_CM=make
CFT_01_WD=.
CFT_02_LB=_Clean
CFT_02_CM=make clean
CFT_02_WD=.
filetypes=C;C++;
NF_02_LB=Make _Object
NF_02_CM=make %e.o
NF_02_WD=
EX_00_LB=_Execute
EX_00_CM=arm-none-eabi-gdb -tui console.elf
EX_00_WD=.
C++FT_01_LB=_Build
C++FT_01_CM=make -j4
C++FT_01_WD=.

[VTE]
last_dir=/home/pi
